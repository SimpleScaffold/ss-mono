{
  "name": "@repo/fe-ui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./button": {
      "import": "./src/exports/button.ts",
      "types": "./src/exports/button.ts"
    },
    "./card": {
      "import": "./src/exports/card.ts",
      "types": "./src/exports/card.ts"
    },
    "./drawer": {
      "import": "./src/exports/drawer.ts",
      "types": "./src/exports/drawer.ts"
    },
    "./dialog": {
      "import": "./src/exports/dialog.ts",
      "types": "./src/exports/dialog.ts"
    },
    "./styles": "./src/styles/index.css",
    "./styles/index.css": "./src/styles/index.css",
    "./styles/palette.css": "./src/styles/palette.css",
    "./styles/tokens.css": "./src/styles/tokens.css",
    "./styles/variables.css": "./src/styles/variables.css",
    "./styles/base.css": "./src/styles/base.css",
    "./styles/*": "./src/styles/*.css"
  },
  "sideEffects": [
    "**/*.css"
